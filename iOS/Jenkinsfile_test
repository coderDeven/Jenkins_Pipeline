pipeline {
    agent { label 'deven' }
    stages {
        stage('Stage One') {
            steps {
                echo "build iOS"
            }
        }
        stage('Setup Job Param'){
            sh "python3 iOS/Params/JenkinsJobParam.py ${game_id} ${push_enable} ${firebase_enable} ${params}"
        }
        stage('Stage getParam') {
            steps {
                // sh './iOS/Sdebug.sh' $game_id $push_enable $token
                echo "woskspace : ${WORKSPACE}"
                echo "gameid : ${params.game_id}"
                sh "./iOS/Sdebug.sh ${params.game_id} ${params.token} ${params.push_enable}"
                sh "./iOS/Sdebug.sh ${game_id} ${token} ${push_enable}"
                sh "python3 iOS/Params/ParamCheck.py ./ddCommon.json"
            }
        }
    }
}